[test text]
navigate to "https://www.google.com"
wait until exists name=q
type "foo" into name=q
sleep 1
type down into name=q
sleep 1
type down into name=q
sleep 1
type up into name=q
sleep 1
type return into name=q
[endtest]

[after all]
